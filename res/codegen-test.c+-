void main() {
	print 4.67
	int n = 35
	print fib(n)
	print fibdp(n)
	
	int fac = 12
	print factr(fac)
	print fact(fac)
}

int fib(int n) {
	if(n < 3)
		return 1
		
	return fib(n - 1) + fib(n - 2)
}

int factr(int n) {
	if(n == 0) return 1
	return n * factr(n - 1)
}

int fact(int n) {
	int i
	for(i = n - 1; i > 1; i--)
		n *= i
	return n
}

int fibdp(int n) {
	int f1 = 1
	int f2 = 1
	int f3 = 1
	
	int i
	for(i = 2; i < n; i++) {
		f3 = f1 + f2
		f1 = f2
		f2 = f3
	} 
	
	return f3
}