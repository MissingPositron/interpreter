//fibonacci yeee

void main() {

	int fib = 10
	
	print "fibonacci:\n"
	
	print "fib(" + fib + ") = "
	print fib(fib) + "\n"
	
	//exponentiation by squaring
	print "\nexponentiation by squaring:\n"
	
	float base = 3
	float exp = 2

	print "exp(" + base + ", " + exp + ") = "

	print exp(base, exp) + "\n"
}

//recursive fibonacci, because now we can!
int fib(int n) {
	if(n == 0 || n == 1)
		return n
		
	return fib(n - 1) + fib(n - 2)
}

//exponentiation by squaring
float exp(float base, float exp) {
	float y = 1
	if(exp < 0) {
		base = 1 / base
		exp = -exp
	}
	
	if(exp != 0) {
		while(exp > 1) {
			if(exp % 2 == 0) {
				base *= base
				exp /= 2
			} else {
				y *= base
				base *= base
				exp = (exp - 1) / 2
			}
		}
		return base * y
	} else
		return 1
}