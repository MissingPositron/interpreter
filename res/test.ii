	int n = 6
	print "fib(" + n + ")\n"
	
	int fn[3]
	fn[0] = 1
	fn[1] = 1
	
	int i
	for(i = 2; i < n; i++) {
		fn[i % 3] = fn[(i - 1) % 3] + fn[(i - 2) % 3]
	}
	
	print fn[(n - 1) % 3] + "\n"
	
	//exponentiation by squaring
	print "\nexponentiation by squaring:\n"
	
	float base = 3
	float exp = 4
	
	print "base: " + base + " exp: " + exp + "\n"
	
	float y = 1
	if(exp < 0) {
		base = 1 / base
		exp = -exp
	}
	
	if(exp != 0) {
		while(exp > 1) {
			if(exp % 2 == 0) {
				base *= base
				exp /= 2
			} else {
				y *= base
				base *= base
				exp = (exp - 1) / 2
			}
		}
		print base * y + "\n"
	} else
		print 1 + "\n"

	return 1
}

